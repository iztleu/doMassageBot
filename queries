CREATE TABLE massageBookingList (
  id    SERIAL PRIMARY KEY,
  mType VARCHAR(100),
  mDate  VARCHAR(100),
  mTime VARCHAR(100),
  userId int,
  status int
);

select * from massageBookingList;
drop table massageBookingList;

select * from massageSchedule;
